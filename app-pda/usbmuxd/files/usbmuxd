# $Header: $

depend() {
	need devfs
}

start() {
    ebegin "Starting usbmuxd"
    start-stop-daemon --start --exec /usr/sbin/usbmuxd -- -u -v -v
    eend $?
}

stop() {
    ebegin "Stopping usbmuxd"
    start-stop-daemon --stop --pidfile /var/run/usbmuxd.pid
    eend $?
}


restart() {
    stop
    start
}
